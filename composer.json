{
  "name": "plaisio/canonical-hostname-resolver-http-host",
  "description": "PhpPlaisio: Canonical Hostname Resolver using HTTP_HOST",
  "keywords": [
    "PhpPlaisio",
    "Canonical Hostname Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "plaisio/canonical-hostname-resolver": "^1.4.0",
    "plaisio/exception": "^1.3.0",
    "plaisio/kernel": "^3.3.0"
  },
  "require-dev": {
    "ext-mbstring": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/console-kernel": "^1.0.0",
    "plaisio/request-core": "^0.13.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
