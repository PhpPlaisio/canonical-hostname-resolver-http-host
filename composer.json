{
  "name": "plaisio/canonical-hostname-resolver-http-host",
  "description": "PhpPlaisio: Canonical Hostname Resolver using HTTP_HOST",
  "keywords": [
    "PhpPlaisio",
    "Canonical Hostname Resolver"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/canonical-hostname-resolver": "^1.1",
    "plaisio/exception": "^1.1"
  },
  "require-dev": {
    "ext-mbstring": "*",
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\CanonicalHostnameResolver\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
